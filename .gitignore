pom.xml
# Ignore OS generated files
.DS_Store           # macOS
Thumbs.db           # Windows

# Ignore IDE/editor specific files
.idea/              # JetBrains IDEs (e.g., IntelliJ, WebStorm)
.vscode/            # Visual Studio Code
*.sublime-workspace # Sublime Text
*.sublime-project   # Sublime Text

# Ignore node_modules for Node.js projects
node_modules/

# Ignore build directories
dist/
build/

# Ignore logs and temporary files
*.log
*.tmp
*.swp              # Swap files for Vim

# Ignore environment files
.env               # .env files for environment variables

# Ignore package lock files (if you don't want to track them)
# package-lock.json
# yarn.lock

# Ignore test and coverage directories
coverage/
test/

# Ignore compiled files
*.class            # Java compiled files
*.exe              # Executables
*.dll              # DLL files

# Ignore Python bytecode files
__pycache__/
*.pyc
*.pyo

# Ignore macOS specific files
.AppleDouble
.LSOverride

# Ignore JetBrains project files
*.iml
*.ipr
*.iws

# Ignore temporary files created by text editors
*~
